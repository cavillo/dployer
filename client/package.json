{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/node": "11.13.4",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.2",
    "axios": "0.18.0",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "tslint": "5.15.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-prod": "npm run clean-conf && npm run build-conf && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean-conf": "rm -f ./src/conf/index.ts",
    "build-conf": "echo \"// @ts-ignore\" >> ./src/conf/index.ts && echo \"export default { apiUrl: '${DPLOYER_API_URL}' !== '' ? '${DPLOYER_API_URL}' : 'http://localhost:8002/api' };\" >> ./src/conf/index.ts",
    "lint": "node_modules/.bin/tslint --project ./tsconfig.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "tslint-react": "4.0.0"
  }
}
